.pagination-block

    ul.pagination

        li.page-item.disabled
            a.page-link(href="#" tabindex="-1") Previous

        li.page-item.active
            a.page-link(href="#") 1
                span.sr-only (current)

        li.page-item
            a.page-link(href="#") 2

        li.page-item
            a.page-link(href="#") 3

        li.page-item
            a.page-link(href="#" tabindex="+1") Next
